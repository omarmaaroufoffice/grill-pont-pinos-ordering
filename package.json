{
  "name": "grill-pont-pinos-ordering",
  "version": "1.0.0",
  "description": "Restaurant ordering system for Grill at Pont Pinos special event",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "start": "node server/index.js",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "dependencies": {
    "concurrently": "^8.2.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "nodemon": "^3.0.1",
    "qrcode": "^1.5.4",
    "socket.io": "^4.7.2",
    "uuid": "^9.0.0"
  },
  "keywords": [
    "restaurant",
    "ordering",
    "menu",
    "qr-code"
  ],
  "author": "Omar",
  "license": "MIT"
}
